package es.jllopezalvarez.programacion.ut03.ejemplos;

public class Ejemplo02DeclararVariables {

	public static void main(String[] args) {
		// Declaración de una variable entera
		int numero;
		numero = 5;
		System.out.println(numero);
		numero = 7;
		System.out.println(numero);

		// Declaración de una variable entera, asignando un valor
		int clase = 3;

		// Mostrar el valor de las variables enteras en la consola. Será necesario dar
		// valor a la primera variable para poder utilizarla.
		System.out.println(clase);
		

		// Declaración de una variable de tipo real (double)

		// Declaración de una variable de tipo real (double), asignando un valor.

		// Mostrar el valor de las variables en la consola. Será necesario dar valor a
		// la primera variable para poder utilizarla.
		
		// Declaración de una variable char

		// Declaración de una variable char, asignando un valor
		char letra = 'f';

		// Mostrar el valor de las variables char en la consola. Será necesario dar
		// valor a la primera variable para poder utilizarla.

		// Declaración de una variable String
		

		// Declaración de una variable String, asignando un valor
		String cadena = "José Luis";
		
		// Mostrar el valor de las variables String en la consola. Será necesario dar
		// valor a la primera variable para poder utilizarla.
		
		int x = 0;
		
	}

}
